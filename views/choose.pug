extends layout

block content
  h1= title
  - var image_names = ['A1.jpg', 'A2.jpg','A3.jpg','A4.jpg','A5.jpg']
  - for (let i = 0; i <= 2; i++)
    - var randomImage = Math.floor(Math.random() * 5)
    - var imgs = image_names[randomImage]
    img(src=`/images/${imgs}`, alt=imgs, width='300', height='300')